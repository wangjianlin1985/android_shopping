<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chengxusheji.domain">

	<class name="MemberInfo" table="MemberInfo">

          <!-- 主键会员用户名 -->
          <id name="memberUserName" type="string" length="20">
            <generator class="assigned"/>
          </id>

          <!-- 简单字符串属性: 登录密码 -->
          <property
            name="password"
            column="password"
            type="string"
            length="20"
          />

          <!-- 简单字符串属性: 真实姓名 -->
          <property
            name="realName"
            column="realName"
            type="string"
            length="20"
          />

          <!-- 简单字符串属性: 性别 -->
          <property
            name="sex"
            column="sex"
            type="string"
            length="2"
          />

          <!-- 简单日期型属性: 出生日期 -->
          <property
            name="birthday"
            column="birthday"
            	type="timestamp"
            	length="10"
          />

          <!-- 简单字符串属性: 联系电话 -->
          <property
            name="telephone"
            column="telephone"
            type="string"
            length="20"
          />

          <!-- 简单字符串属性: 联系邮箱 -->
          <property
            name="email"
            column="email"
            type="string"
            length="40"
          />

          <!-- 简单字符串属性: 联系qq -->
          <property
            name="qq"
            column="qq"
            type="string"
            length="20"
          />

          <!-- 简单字符串属性: 家庭地址 -->
          <property
            name="address"
            column="address"
            type="string"
            length="60"
          />

          <!-- 简单图片属性: 会员照片 -->
          <property
            name="photo"
            column="photo"
            type="string"
            length="50"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
